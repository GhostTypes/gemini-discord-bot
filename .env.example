# Discord Bot Configuration
DISCORD_TOKEN=your_discord_bot_token_here
DISCORD_CLIENT_ID=your_discord_client_id_here

# Google AI Configuration
GOOGLE_AI_API_KEY=your_google_ai_api_key_here
GOOGLE_AI_MODEL=gemini-2.5-flash-lite

# Thinking Configuration
THINKING_ENABLED=false
THINKING_BUDGET=-1

# Database Configuration (shouldn't need to change)
DATABASE_URL="file:./message_cache.db"

# Message Cache Configuration
MESSAGE_CACHE_SIZE=64

# RAG Configuration
RAG_ENABLED=true
RAG_MAX_CONTEXT_MESSAGES=12
RAG_RELEVANCE_THRESHOLD=0.3

# Mapillary Configuration (for GeoGuesser images)
# Get your access token from: https://www.mapillary.com/developer
MAPILLARY_ACCESS_TOKEN=YOUR_TOKEN_HERE
MAPILLARY_ENABLED=true